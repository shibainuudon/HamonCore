			<IndexListItems>
				<Size>$T2</Size>
				<ValueNode>(m_value &gt;&gt; $i) &amp; 1</ValueNode>
			</IndexListItems>
		</Expand>
	</Type>
	<Type Name="hamon::bitset_detail::bitset_impl&lt;*,0,0&gt;">
		<DisplayString>[empty]</DisplayString>
	</Type>
	<Type Name="hamon::bitset&lt;*&gt;">
		<DisplayString>{m_impl}</DisplayString>
