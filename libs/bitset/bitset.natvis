		<Expand>
			<IndexListItems>
				<Size>$T2</Size>
				<ValueNode>(m_value[$i / BitsPerWord] &gt;&gt; ($i % BitsPerWord)) &amp; 1</ValueNode>
			</IndexListItems>
		</Expand>
	</Type>
	<Type Name="hamon::bitset_detail::bitset_impl&lt;*,*,1&gt;">
		<DisplayString>{{ size={$T2} }}</DisplayString>
